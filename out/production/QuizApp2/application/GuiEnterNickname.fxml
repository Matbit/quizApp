<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="172.0" prefWidth="371.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GuiEnterNicknameController">
   <children>
      <Label layoutX="96.0" layoutY="22.0" text="Gebe hier deinen Namen ein:">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="tbxNickname" layoutX="53.0" layoutY="59.0" onInputMethodTextChanged="#txtfieldChangetxtfieldChange" onKeyTyped="#txtfieldChangetxtfieldChange" prefHeight="27.0" prefWidth="266.0" promptText="max. 20 Zeichen erlaubt" />
      <Button fx:id="btnGo" layoutX="134.0" layoutY="109.0" mnemonicParsing="false" onAction="#buttonClicked" prefHeight="27.0" prefWidth="106.0" text="Fertig!">
         <font>
            <Font name="Courier" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
